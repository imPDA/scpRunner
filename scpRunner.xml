<GuiXml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sir.insidi.at/or/schema/esoui.xsd">
    <Controls>
        <!-- Virtual controls -->
        <Label name="scpDefaultLabel" font="ZoFontWinH1" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL" horizontalAlignment="LEFT" verticalAlignment="CENTER" virtual="true" />
    
        <Control name="scpRowTemplate" virtual="true">
            <Dimensions y="40" />
            <Controls>
                <Label name="$(parent)NameLabel" inherits="scpDefaultLabel" font="ZoFontWinH2" text="-">
                    <Anchor point="LEFT" />
                </Label>
                <Label name="$(parent)TimeLabel" inherits="scpDefaultLabel" font="ZoFontWinH3" text="-">
                    <Anchor point="LEFT" relativeTo="$(parent)NameLabel" relativePoint="RIGHT" offsetX="15" />
                </Label>
            </Controls>
        </Control>
        
        <!-- Timer and Splits visualizer -->
        <TopLevelControl name="scprHudTimer" resizeToFitDescendents="true" mouseEnabled="true" movable="true" clampedToScreen="true">
            <Controls>
				<Texture name="$(parent)Icon" textureFile="/esoui/art/miscellaneous/timer_32.dds" hidden="true">
					<Anchor point="TOPLEFT" offsetX="100" offsetY="100"/>
					<Dimensions x="32" y="32"/>
				</Texture>
				<Label name="$(parent)Timer" font="ZoFontWinH2" horizontalAlignment="LEFT" verticalAlignment="CENTER" hidden="true">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Icon" offsetX="6" offsetY="-1"/>
					<Dimensions y="24"/>
				</Label>
				<Texture name="$(parent)SplitIcon" textureFile="/esoui/art/miscellaneous/gamepad/gp_scrollarrow_up.dds" alpha="0">
					<Anchor point="TOPLEFT" offsetX="120" offsetY="136"/>
					<Dimensions x="20" y="20"/>
				</Texture>
				<Label name="$(parent)SplitTime" font="ZoFontWinH4" horizontalAlignment="LEFT" verticalAlignment="CENTER" alpha="0">
					<Anchor point="TOPLEFT" offsetX="120" offsetY="136"/>
					<Dimensions y="24"/>
				</Label>
            </Controls>
		</TopLevelControl>

        <Texture name="scpDebugBackground" color="000011" alpha="0.5" virtual="true">
            <AnchorFill />
        </Texture>

        <!-- Main run review UI -->
        <TopLevelControl name="scprStatsUI" hidden="false">
            <Dimensions x="1152" y="1075" />
            <Anchor point="RIGHT" offsetX="-50" />
                <Controls>
                    <Texture name="$(parent)DebugBackground" inherits="scpDebugBackground" color="005500" alpha="0.5" />
                    <Control name="$(parent)Header">
                        <Dimensions x="404" y="75" />
                        <Anchor point="TOP" />
                        <Controls>
                            <!-- <Texture name="$(parent)DebugBackground" inherits="scpDebugBackground" /> -->
                            <Texture name="$(parent)TopTopDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds" tier="HIGH">
                                <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="TOP" offsetY="2" />
                                <Dimensions x="440" y="24" />
                            </Texture>
                            <Texture name="$(parent)loadscreenBG" textureFile="ScpRunner/art/ScpLoadscreenCrop.dds" tier="LOW">
                                <AnchorFill />
                            </Texture>
                            <Label name="$(parent)DungeonName" inherits="scpDefaultLabel" text="Scalecaller Peak">
                                <Anchor point="CENTER" offsetX="42" />
                            </Label>
                            <Texture name="$(parent)DungeonIcon" textureFile="/esoui/art/journal/gamepad/gp_questtypeicon_groupdungeon.dds">
                                <Dimensions y="64" x="64"/>
                                <Anchor point="RIGHT" relativeTo="$(parent)DungeonName" relativePoint="LEFT" offsetX="-20" />
                            </Texture>
                            <Texture name="$(parent)TopDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds" tier="HIGH">
                                <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="BOTTOM" offsetY="2" />
                                <Dimensions x="1152" y="24"/>
                            </Texture>
                        </Controls>
                    </Control>
                    <Control name="$(parent)Body">
                        <Dimensions x="1152" y="1000" />
                        <Anchor point="TOP" relativeTo="$(parent)Header" relativePoint="BOTTOM" />
                        <Controls>
                            <Texture name="$(parent)TopBG" textureFile="ScpRunner/art/PostDungeonBG1.dds" alpha="0.7" tier="LOW">
                                <Dimensions x="1152" y="700"/>
                                <Anchor point="TOP" /> 
                            </Texture>
                            <Texture name="$(parent)BottomBG" textureFile="ScpRunner/art/PostDungeonBG2.dds" alpha="0.7" tier="LOW">
                                <Dimensions x="1152" y="700"/>
                                <Anchor point="BOTTOM" />
                            </Texture>
                            <Control name="$(parent)CentralColumn">
                                <Dimensions x="390" y="900" />
                                <Anchor point="TOP" />
                                <Controls>
                                    <!-- <Texture name="$(parent)DebugBackground" inherits="scpDebugBackground" /> -->
                                    <Control name="$(parent)TimeTally" resizeToFitDescendents="true">
                                        <Anchor point="TOP" offsetY="10" />
                                        <Controls>
                                            <Texture name="$(parent)Icon" textureFile="/esoui/art/trials/trialpoints_veryhigh.dds">
                                                <Anchor point="LEFT" />
                                                <Dimensions x="54" y="54"/>
                                            </Texture>
                                            <Label name="$(parent)Label" inherits="scpDefaultLabel" text="00:00.000">
                                                <Anchor point="LEFT" relativeTo="$(parent)Icon" relativePoint="RIGHT" offsetX="10" />
                                            </Label>
                                        </Controls>
                                    </Control>
                                    <Texture name="$(parent)TopSubDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds" tier="HIGH">
                                        <Dimensions x="390" y="24"/>
                                        <Anchor point="TOP" relativeTo="$(parent)TimeTally" relativePoint="BOTTOM" />
                                    </Texture>
                                    <Texture name="$(parent)Map" textureFile="ScpRunner/art/SCP-Map-Clean.dds" movable="true" mouseEnabled="true" alpha="0.6" tier="HIGH">
                                        <Dimensions x="350" y="672" />
                                        <Anchor point="CENTER" />
                                    </Texture>
                                    <Texture name="$(parent)BottomSubDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds" tier="HIGH">
                                        <Dimensions x="460" y="24"/>
                                        <Anchor point="CENTER" relativePoint="BOTTOM" />
                                    </Texture>
                                </Controls>
                            </Control>
                            <Control name="$(parent)Splits">
                                <Dimensions x="300" y="900"/>
                                <Anchor point="RIGHT" relativeTo="$(parent)CentralColumn" relativePoint="LEFT" offsetX="-20" />
                                <Controls>
                                    <!-- <Texture name="$(parent)DebugBackground" inherits="scpDebugBackground" color="550055" alpha="0.5" /> -->
                                    <Control name="$(parent)Header" resizeToFitDescendents="true">
                                        <Anchor point="TOP" offsetY="95"/>
                                        <Controls>
                                            <Texture name="$(parent)Icon" textureFile="/esoui/art/compass/repeatablequest_icon_assisted.dds">
                                                <Dimensions x="48" y="48" />
                                                <Anchor point="LEFT" />
                                            </Texture>
                                            <Label name="$(parent)Label" inherits="scpDefaultLabel" text="Splits">
                                                <Anchor point="LEFT" relativeTo="$(parent)Icon" relativePoint="RIGHT" offsetX="10" />
                                            </Label>
                                        </Controls>
                                    </Control>
                                    <Texture name="$(parent)TopDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds" tier="HIGH">
                                        <Dimensions x="230" y="24" />
                                        <Anchor point="BOTTOM" relativeTo="$(parent)Header" relativePoint="TOP" />
                                    </Texture>
                                    <Texture name="$(parent)BottomDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds" tier="HIGH">
                                        <Dimensions x="230" y="24" />
                                        <Anchor point="TOP" relativeTo="$(parent)Header" relativePoint="BOTTOM" />
                                    </Texture>
                                    <Control name="$(parent)List" inherits="ZO_ScrollList">
                                        <Dimensions x="350" />
                                        <Anchor point="TOP" relativeTo="$(parent)BottomDivider" relativePoint="BOTTOM" />
                                        <Anchor point="BOTTOM" />
                                        <Controls>
                                            <!-- <Texture name="$(parent)DebugBackground" inherits="scpDebugBackground" color="00FF00" alpha="0.5" /> -->
                                        </Controls>
                                    </Control>
                                </Controls>
                            </Control>
                            <Control name="$(parent)Trifectas">
                                <Dimensions x="300" y="900" />
                                <Anchor point="LEFT" relativeTo="$(parent)CentralColumn" relativePoint="RIGHT" offsetX="20" />
                                <Controls>
                                    <!-- <Texture name="$(parent)DebugBackground" inherits="scpDebugBackground" color="550055" alpha="0.5" /> -->
                                    <Control name="$(parent)Header" resizeToFitDescendents="true">
                                        <Anchor point="TOP" offsetY="95" />
                                        <Controls>
                                            <Texture name="$(parent)Icon" textureFile="/esoui/art/menubar/gamepad/gp_playermenu_icon_achievements.dds">
                                                <Dimensions x="48" y="48"/>
                                                <Anchor point="LEFT" />
                                            </Texture>
                                            <Label name="$(parent)Label" inherits="scpDefaultLabel" text="Trifectas">
                                                <Anchor point="LEFT" relativeTo="$(parent)Icon" relativePoint="RIGHT" offsetX="10" />
                                            </Label>
                                        </Controls>
                                    </Control>
                                    <Texture name="$(parent)TopDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds">
                                        <Dimensions x="230" y="24"/>
                                        <Anchor point="BOTTOM" relativeTo="$(parent)Header" relativePoint="TOP" />
                                    </Texture>
                                    <Texture name="$(parent)BottomDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds" tier="HIGH">
                                        <Dimensions x="230" y="24" />
                                        <Anchor point="TOP" relativeTo="$(parent)Header" relativePoint="BOTTOM" />
                                    </Texture>
                                </Controls>
                            </Control>
                        </Controls>
                    </Control>
                    <Texture name="$(parent)BottomDivider" textureFile="/esoui/art/miscellaneous/horizontaldivider_dynamic.dds" tier="HIGH">
                        <Dimensions x="1152" y="24"/>
                        <Anchor point="CENTER" relativeTo="$(parent)Body" relativePoint="BOTTOM" />
                    </Texture>
                </Controls>
        </TopLevelControl>
    </Controls>
</GuiXml>